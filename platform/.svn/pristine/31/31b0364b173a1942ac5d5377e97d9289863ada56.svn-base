body { /*IE居中*/
	text-align: left;
	font: 12px/1.5 tahoma, arial, sans-serif;
	margin: 0;
	padding: 0;
	margin-left: auto;
	margin-right: auto;
}
input {
	font-family: '宋体'
}
.top {
	width: 80%;
	/*FF居中*/
	
}

