<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<package name="json" namespace="/json" extends="json-default">
		<action name="checkUsername" class="userAction" method="checkUsername">
			<result type="json" />
		</action>
		<action name="checkEmail" class="userAction" method="checkEmail">
			<result type="json" />
		</action>
		<action name="searchUsername" class="userAction" method="searchUsername" />
		<action name="sendMobileVerifyCode" class="userAction"
			method="sendMobileVerifyCode" />
		<action name="sendVerifyCode" class="forgotpassAction" method="sendVerifyCode" />
		<action name="portrait_delete" class="adminAction" method="portrait_delete">
			<result type="json" />
		</action>
	</package>
</struts>
