package cc.ywxm.model.game;

import java.sql.Timestamp;

public class BusinessBuilding
{

	// Fields

	private Integer player;
	private Integer id;
	private Short kind;
	private Short x;
	private Short y;
	private Short aspect;
	private Boolean warehoused;
	private Short progress;
	private Timestamp lastReap;
	public Integer getPlayer()
	{
		return player;
	}
	public void setPlayer(Integer player)
	{
		this.player = player;
	}
	public Integer getId()
	{
		return id;
	}
	public void setId(Integer id)
	{
		this.id = id;
	}
	public Short getKind()
	{
		return kind;
	}
	public void setKind(Short kind)
	{
		this.kind = kind;
	}
	public Short getX()
	{
		return x;
	}
	public void setX(Short x)
	{
		this.x = x;
	}
	public Short getY()
	{
		return y;
	}
	public void setY(Short y)
	{
		this.y = y;
	}
	public Short getAspect()
	{
		return aspect;
	}
	public void setAspect(Short aspect)
	{
		this.aspect = aspect;
	}
	public Boolean getWarehoused()
	{
		return warehoused;
	}
	public void setWarehoused(Boolean warehoused)
	{
		this.warehoused = warehoused;
	}
	public Short getProgress()
	{
		return progress;
	}
	public void setProgress(Short progress)
	{
		this.progress = progress;
	}
	public Timestamp getLastReap()
	{
		return lastReap;
	}
	public void setLastReap(Timestamp lastReap)
	{
		this.lastReap = lastReap;
	}
	

	
}