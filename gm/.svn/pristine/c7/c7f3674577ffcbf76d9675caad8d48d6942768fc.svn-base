package cc.ywxm.model.platform;

/**
 * 玩家职业统计
 * 
 * @author HuangDeCai
 * @since 2012-12-20 13:20:22
 * 
 */
public class CareerCount
{
	private String career;
	private int total;

	public CareerCount()
	{
		super();
	}

	public CareerCount(String career, int total)
	{
		super();
		this.career = career;
		this.total = total;
	}

	public String getCareer()
	{
		return career;
	}

	public void setCareer(String career)
	{
		this.career = career;
	}

	public int getTotal()
	{
		return total;
	}

	public void setTotal(int total)
	{
		this.total = total;
	}

}
